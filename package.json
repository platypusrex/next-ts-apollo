{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "tslint -p tslint.json && next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "lint:ts": "tslint -p tslint.json --fix",
    "generate:types": "apollo client:codegen src/typings/generated.ts --outputFlat --target=typescript --endpoint=http://localhost:4000/graphql --includes=./src/graphql/**/*.graphql --mergeInFieldsFromFragmentSpreads",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:4000/graphql --includes=./src/graphql/**/*.graphql"
  },
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "apollo-mocked": "^0.2.3",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.0.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.5",
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "apollo": "^2.16.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "jest": "^24.8.0",
    "next-plugin-graphql": "^0.0.2",
    "tslint": "^5.18.0",
    "typescript": "3.5.3"
  },
  "license": "ISC"
}
